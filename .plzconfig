[Please]
Version = 15.0.0

[Build]
PassEnv = PATH
PassEnv = USER

[Alias "genReport"]
cmd = build //src/cockpit/scripts:gen_report

[Alias "genComponent"]
cmd = run //src/cockpit/scripts:gen_component

[Alias "pull"]
cmd = run //scripts:pull

[Alias "reset"]
cmd = run //scripts:reset

[Alias "show"]
cmd = run //:serve

[Alias "work"]
cmd = run //src/turbine:main

[go]
importpath = github.com/binhonglee/GlobeTrotte
