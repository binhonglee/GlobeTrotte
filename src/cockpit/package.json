{
  "name": "@gt/cockpit",
  "version": "0.0.1",
  "private": "true",
  "author": "BinHong Lee <binhong@binhong.me>",
  "scripts": {
    "build": "vite build",
    "check:vue_tsc": "vue-tsc --noEmit --skipLibCheck",
    "checkBackend": "esbuild --bundle scripts/checkBackend.ts --platform=node --outdir=./plz-out && node ./plz-out/checkBackend.js",
    "format": "pnpm run lint --fix",
    "lint": "eslint \"**/*.{vue,ts}\"",
    "preflight": "pnpm i && pnpm run genRouter && mv router.ts ",
    "serve": "pnpm run checkBackend && vite",
    "startServer": "forever start node_modules/vite/bin/vite.js",
    "test:cypress": "pnpm run checkBackend && NODE_ENV=test cypress install && cypress run",
    "test:cypress:ct": "cypress run-ct --config-file cypress-ct.json",
    "test:cypress:plz": "pnpm run xvfb:run && $xvfb pnpm run test:cypress",
    "test:vitest": "vitest",
    "test:playwright": "vitest -c configs/vitest_playwright.config.ts",
    "test:circleci": "vitest -c configs/vitest_circleci.config.ts",
    "xvfb:run": "if [[ \"$OSTYPE\" == \"darwin\"* ]]; then xvfb=\"\"; elif [[ \"$OSTYPE\" == \"linux-gnu\"* ]]; then xvfb=\"xvfb-run -a \"; fi"
  },
  "dependencies": {
    "@vueuse/head": "0.7.6",
    "axios": "0.27.2",
    "naive-ui": "2.27.0",
    "vue": "3.2.33",
    "wings-ts-util": "0.0.8"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.12",
    "@cypress/vite-dev-server": "2.2.3",
    "@cypress/vue": "3.1.2",
    "@delagen/eslint-plugin-deprecation": "1.3.2",
    "@types/node": "16.11.36",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "@vicons/ionicons5": "0.12.0",
    "@vitejs/plugin-vue": "2.3.2",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "2.0.0",
    "c8": "7.11.3",
    "cypress": "9.6.1",
    "esbuild": "0.14.38",
    "eslint": "8.15.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.7.1",
    "jest-serializer-html": "7.1.0",
    "jsdom": "19.0.0",
    "playwright-core": "1.22.2",
    "prettier": "2.6.2",
    "typescript": "4.6.4",
    "vite": "2.9.8",
    "vite-plugin-pwa": "0.12.0",
    "vitest": "0.12.9",
    "vue-router": "4.0.15",
    "vue-tsc": "0.34.11",
    "workbox-window": "6.5.3"
  },
  "license": "AGPL 3.0"
}
