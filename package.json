{
  "name": "globetrotte",
  "version": "1.0.0",
  "private": true,
  "author": "BinHong Lee <binhong@binhong.me>",
  "scripts": {
    "serve": "pnpm run checkBackend && vite",
    "vite": "vite",
    "build": "vite build",
    "lint": "eslint \"src/**/*.{vue,ts}\"",
    "check:tsc": "vue-tsc --noEmit --skipLibCheck",
    "checkBackend": "esbuild --bundle src/cockpit/scripts/checkBackend.ts --platform=node --outdir=./plz-out && node ./plz-out/checkBackend.js",
    "endServer": "forever stop node_modules/vite/bin/vite.js",
    "format": "pnpm run lint -- --fix",
    "preflight": "pnpm i && pnpm run genRouter && mv router.ts src/cockpit/",
    "postinstall": "touch node_modules/go.mod",
    "startServer": "forever start node_modules/vite/bin/vite.js",
    "test:cypress": "pnpm run checkBackend && NODE_ENV=test cypress install && cypress run",
    "test:cypress:ct": "cypress run-ct --config-file cypress-ct.json",
    "test:cypress:plz": "pnpm run xvfb:run && $xvfb pnpm run test:cypress",
    "test:vitest": "vitest",
    "report": "nyc merge plz-out/gen/src/cockpit/scripts/.nyc_output .nyc_output/coverage.json && nyc report",
    "xvfb:run": "if [[ \"$OSTYPE\" == \"darwin\"* ]]; then xvfb=\"\"; elif [[ \"$OSTYPE\" == \"linux-gnu\"* ]]; then xvfb=\"xvfb-run -a \"; fi"
  },
  "dependencies": {
    "@vueuse/head": "0.7.6",
    "axios": "0.27.2",
    "naive-ui": "2.27.0",
    "vue": "3.2.33",
    "wings-ts-util": "0.0.8"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.12",
    "@cypress/vite-dev-server": "2.2.2",
    "@cypress/vue": "3.1.1",
    "@delagen/eslint-plugin-deprecation": "1.3.2",
    "@types/node": "16.11.33",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "@vicons/ionicons5": "0.12.0",
    "@vitejs/plugin-vue": "2.3.1",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "2.0.0-rc.21",
    "c8": "7.11.2",
    "cypress": "9.6.0",
    "esbuild": "0.14.38",
    "eslint": "8.14.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.7.1",
    "forever": "4.0.3",
    "jest-serializer-html": "7.1.0",
    "jsdom": "19.0.0",
    "nyc": "15.1.0",
    "playwright-core": "1.21.1",
    "prettier": "2.6.2",
    "pug": "3.0.2",
    "typescript": "4.6.4",
    "vite": "2.9.7",
    "vite-plugin-pwa": "0.12.0",
    "vitest": "0.10.1",
    "vue-router": "4.0.14",
    "vue-tsc": "0.34.11"
  },
  "license": "AGPL 3.0"
}
