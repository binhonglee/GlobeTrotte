{
  "name": "globetrotte",
  "version": "1.0.0",
  "private": true,
  "author": "BinHong Lee <binhong@binhong.me>",
  "scripts": {
    "serve": "pnpm run checkBackend && vite",
    "build": "vite build",
    "lint": "eslint \"src/**/*.{vue,ts}\"",
    "check:tsc": "vue-tsc --noEmit --skipLibCheck",
    "checkBackend": "esbuild --bundle src/cockpit/scripts/checkBackend.ts --platform=node --outdir=./plz-out && node ./plz-out/checkBackend.js",
    "endServer": "forever stop node_modules/vite/bin/vite.js",
    "format": "pnpm run lint -- --fix",
    "preflight": "pnpm i && pnpm run genRouter && mv router.ts src/cockpit/",
    "postinstall": "touch node_modules/go.mod",
    "startServer": "forever start node_modules/vite/bin/vite.js",
    "test:ava": "pnpm run preflight && nyc ava",
    "test:cypress": "pnpm run checkBackend && NODE_ENV=test cypress install && cypress run",
    "test:cypress:plz": "pnpm run xvfb:run && $xvfb pnpm run test:cypress",
    "report": "nyc merge plz-out/gen/src/cockpit/scripts/.nyc_output .nyc_output/coverage.json && nyc report",
    "xvfb:run": "if [[ \"$OSTYPE\" == \"darwin\"* ]]; then xvfb=\"\"; elif [[ \"$OSTYPE\" == \"linux-gnu\"* ]]; then xvfb=\"xvfb-run -a \"; fi"
  },
  "dependencies": {
    "axios": "0.24.0",
    "element-plus": "1.0.2-beta.71",
    "vue": "3.2.24",
    "wings-ts-util": "0.0.8"
  },
  "devDependencies": {
    "@ava/babel": "2.0.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/plugin-transform-typescript": "7.16.1",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-typescript": "7.16.0",
    "@cypress/code-coverage": "3.9.12",
    "@cypress/vite-dev-server": "2.2.1",
    "@delagen/eslint-plugin-deprecation": "1.3.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@types/puppeteer": "5.4.4",
    "@types/sinon": "10.0.6",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "@vitejs/plugin-vue": "1.10.2",
    "@vue/compiler-sfc": "3.2.24",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "9.1.0",
    "@vue/test-utils": "2.0.0-rc.17",
    "ava": "3.15.0",
    "babel-loader": "8.2.3",
    "babel-plugin-istanbul": "6.1.1",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-preset-typescript-vue3": "2.0.16",
    "cypress": "9.1.1",
    "esbuild": "0.14.2",
    "esbuild-jest": "0.5.0",
    "eslint": "8.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.2.0",
    "forever": "4.0.1",
    "jest": "26.6.3",
    "jest-junit": "13.0.0",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "naive-ui": "2.21.5",
    "node-sass": "7.0.0",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "pug": "3.0.2",
    "pug-plain-loader": "1.1.0",
    "puppeteer": "12.0.1",
    "puppeteer-to-istanbul": "1.4.0",
    "require-extension-hooks": "0.3.3",
    "require-extension-hooks-babel": "1.0.0",
    "require-extension-hooks-vue": "3.0.0",
    "sass": "1.44.0",
    "sass-loader": "12.4.0",
    "sinon": "12.0.1",
    "tap-xunit": "2.4.1",
    "ts-jest": "26.5.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.2",
    "vite": "2.7.1",
    "vite-plugin-test": "0.0.5",
    "vue-jest": "5.0.0-alpha.10",
    "vue-loader": "16.8.3",
    "vue-router": "4.0.12",
    "vue-tsc": "0.29.8",
    "wait-on": "6.0.0"
  },
  "license": "AGPL 3.0"
}
