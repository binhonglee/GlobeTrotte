{
  "name": "globetrotte",
  "version": "1.0.0",
  "private": true,
  "author": "BinHong Lee <binhong@binhong.me>",
  "scripts": {
    "serve": "pnpm run checkBackend && parcel index.html",
    "build": "parcel build index.html --no-cache --public-url 'https://globetrotte.com'",
    "lint": "eslint \"src/**/*.{vue,ts}\"",
    "check:tsc": "tsc --noEmit",
    "checkBackend": "tsc src/cockpit/scripts/checkBackend.ts --outDir ./plz-out && node ./plz-out/checkBackend.js",
    "endServer": "forever stop ./node_modules/parcel-bundler/bin/cli.js",
    "format": "pnpm run lint -- --fix",
    "preflight": "pnpm i && pnpm run genRouter && mv router.ts src/cockpit/",
    "startServer": "forever start ./node_modules/parcel-bundler/bin/cli.js index.html && wait-on http://localhost:1234",
    "test:ava": "pnpm run preflight && nyc ava",
    "test:cypress": "cypress install && cypress run",
    "test:cypress:plz": "pnpm run checkBackend && NODE_ENV=test pnpm run startServer && pnpm run xvfb:run && $xvfb pnpm run test:cypress && pnpm run endServer",
    "report": "nyc merge plz-out/gen/src/cockpit/scripts/.nyc_output .nyc_output/coverage.json && nyc report",
    "xvfb:run": "if [[ \"$OSTYPE\" == \"darwin\"* ]]; then xvfb=\"\"; elif [[ \"$OSTYPE\" == \"linux-gnu\"* ]]; then xvfb=\"xvfb-run -a \"; fi"
  },
  "dependencies": {
    "axios": "0.21.1",
    "element-ui": "2.15.1",
    "vue": "2.6.12",
    "vue-router": "3.5.1",
    "wings-ts-util": "0.0.8"
  },
  "devDependencies": {
    "@ava/babel": "1.0.1",
    "@babel/core": "7.14.2",
    "@babel/plugin-transform-runtime": "7.14.2",
    "@babel/plugin-transform-typescript": "7.13.0",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-typescript": "7.13.0",
    "@cypress/code-coverage": "3.9.5",
    "@types/node": "14.14.45",
    "@types/puppeteer": "5.4.3",
    "@types/sinon": "10.0.0",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "1.2.0",
    "ava": "3.15.0",
    "babel-loader": "8.2.2",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-preset-typescript-vue": "1.1.1",
    "cypress": "6.9.1",
    "eslint": "7.26.0",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.9.0",
    "forever": "3.0.4",
    "jsdom": "16.5.3",
    "jsdom-global": "3.0.2",
    "node-sass": "6.0.0",
    "nyc": "15.1.0",
    "parcel-bundler": "1.12.5",
    "prettier": "2.3.0",
    "pug": "3.0.2",
    "pug-plain-loader": "1.1.0",
    "puppeteer": "9.1.1",
    "puppeteer-to-istanbul": "1.4.0",
    "require-extension-hooks": "0.3.3",
    "require-extension-hooks-babel": "1.0.0",
    "require-extension-hooks-vue": "3.0.0",
    "sass-loader": "11.1.1",
    "sinon": "10.0.0",
    "tap-xunit": "2.4.1",
    "typescript": "4.2.4",
    "vue-loader": "15.9.7",
    "vue-template-compiler": "2.6.12",
    "wait-on": "5.3.0"
  },
  "license": "AGPL 3.0"
}
