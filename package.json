{
    "name": "globetrotte",
    "productName": "GlobeTrotte",
    "version": "1.0.0",
    "author": "BinHong Lee <binhong@binhong.me>",
    "private": true,
    "license": "AGPL 3.0",
    "scripts": {
        "genRouter": "tsc src/cockpit/scripts/genRouter.ts --outDir ./ && node ./genRouter.js && rm ./genRouter.js",
        "serve": "parcel index.html",
        "build": "parcel build index.html --no-cache --public-url 'https://globetrotte.com'",
        "lint": "tslint --project tsconfig.json",
        "test": "mocha",
        "cover": "nyc mocha"
    },
    "dependencies": {
        "element-ui": "2.11.1",
        "tslib": "1.10.0",
        "vue": "2.6.10",
        "vue-hot-reload-api": "2.3.3",
        "vue-property-decorator": "8.2.1",
        "vue-router": "3.1.2",
        "wings-ts-util": "0.0.6"
    },
    "devDependencies": {
        "@types/assert": "1.4.3",
        "@types/chai": "4.2.0",
        "@types/mocha": "5.2.7",
        "@types/node": "12.7.2",
        "@vue/component-compiler-utils": "3.0.0",
        "chai": "4.2.0",
        "mocha": "6.2.0",
        "nyc": "14.1.1",
        "parcel-bundler": "1.12.3",
        "pug": "2.0.4",
        "sass": "1.22.10",
        "ts-node": "8.6.1",
        "tslint": "5.18.0",
        "typescript": "3.5.3",
        "vue-template-compiler": "2.6.10"
    },
    "nyc": {
        "instrument": true,
        "sourceMaps": false,
        "all": true,
        "check-coverage": false,
        "per-file": true,
        "clean": true,
        "temp-dir": "./.nyc_output",
        "include": [
            "src/cockpit/**/*.{ts,vue}"
        ],
        "exclude": [
            "**/*.{spec,test}.{js,ts}",
            "*.js"
        ],
        "reporter": [
            "lcov",
            "cobertura",
            "text",
            "text-summary"
        ],
        "report-dir": "./coverage",
        "extension": [
            ".ts",
            ".vue"
        ]
    }
}
