{
  "name": "globetrotte",
  "version": "1.0.0",
  "private": true,
  "author": "BinHong Lee <binhong@binhong.me>",
  "scripts": {
    "serve": "parcel index.html",
    "build": "parcel build index.html --no-cache --public-url 'https://globetrotte.com'",
    "lint": "eslint .",
    "cover": "nyc pnpm run test:unit",
    "cypress": "NODE_ENV=test vue-cli-service test:e2e",
    "endServer": "forever stop ./node_modules/@vue/cli-service/bin/vue-cli-service.js",
    "format": "pnpm run lint -- --fix",
    "format:plz": "pnpm run format",
    "genComponent": "tsc src/cockpit/scripts/genComponent.ts --outDir ./plz-out && node ./plz-out/genComponent.js",
    "genRouter": "tsc src/cockpit/scripts/genRouter.ts --outDir ./plz-out && node ./plz-out/genRouter.js",
    "startServer": "forever start parcel index.html && wait-on http://localhost:1234",
    "test:e2e": "cypress run",
    "test:e2e:plz": "NODE_ENV=test pnpm run startServer && pnpm run xvfb:run && $xvfb pnpm run test:e2e && pnpm run endServer",
    "test:component": "vue-cli-service test:unit src/cockpit/tests/components/*.spec.ts --reporter mocha-multi-reporters --reporter-options configFile=componentReporter.json",
    "test:mocha": "nyc mocha",
    "test:mocha:webpack": "nyc mocha-webpack --require src/cockpit/tests/support/setup.js --require ts-node/register --webpack-config src/cockpit/tests/support/webpack.config.js --reporter mocha-multi-reporters --reporter-options configFile=mochaReporter.json \"src/cockpit/tests/**/*.spec.ts\"",
    "xvfb:run": "if [[ \"$OSTYPE\" == \"darwin\"* ]]; then xvfb=\"\"; elif [[ \"$OSTYPE\" == \"linux-gnu\"* ]]; then xvfb=\"xvfb-run -a \"; fi"
  },
  "dependencies": {
    "axios": "0.21.0",
    "element-ui": "2.14.1",
    "tslib": "2.0.3",
    "vue": "2.6.12",
    "vue-class-component": "7.2.6",
    "vue-property-decorator": "9.1.2",
    "vue-router": "3.4.9",
    "wings-ts-util": "0.0.6"
  },
  "devDependencies": {
    "@types/assert": "1.5.2",
    "@types/mocha": "8.0.4",
    "@types/node": "14.10.0",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "@vue/cli-plugin-e2e-cypress": "4.5.9",
    "@vue/cli-plugin-eslint": "4.5.9",
    "@vue/cli-plugin-router": "4.5.9",
    "@vue/cli-plugin-typescript": "4.5.9",
    "@vue/cli-plugin-unit-mocha": "4.5.9",
    "@vue/cli-service": "4.5.9",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "1.1.1",
    "chai": "4.2.0",
    "cross-env": "7.0.3",
    "eslint": "7.15.0",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-vue": "7.2.0",
    "forever": "3.0.4",
    "mocha": "7.2.0",
    "mocha-junit-reporter": "2.0.0",
    "mocha-multi-reporters": "1.5.1",
    "node-sass": "5.0.0",
    "nyc": "15.1.0",
    "parcel-bundler": "1.12.4",
    "prettier": "2.2.1",
    "pug": "3.0.0",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "10.1.0",
    "ts-node": "9.0.0",
    "typescript": "4.1.2",
    "vue-loader": "15.9.5",
    "vue-template-compiler": "2.6.12",
    "wait-on": "5.2.0"
  },
  "license": "AGPL 3.0",
  "nyc": {
    "instrument": true,
    "sourceMaps": false,
    "all": true,
    "check-coverage": false,
    "per-file": true,
    "clean": true,
    "temp-dir": "./.nyc_output",
    "include": [
      "src/cockpit/**/*.{ts,vue}"
    ],
    "exclude": [
      "**/*.{spec,test}.{js,ts}",
      "src/cockpit/router.ts",
      "*.js"
    ],
    "reporter": [
      "cobertura",
      "text",
      "text-summary"
    ],
    "report-dir": "./coverage",
    "extension": [
      ".ts",
      ".vue"
    ]
  }
}
